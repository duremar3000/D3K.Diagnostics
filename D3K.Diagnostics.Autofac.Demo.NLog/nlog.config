<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" autoReload="true">
  <targets>
    <target name="Debug" type="File" fileName="logs\debug.txt" concurrentWrites="true" layout="${longdate} [${mdlc:item=pid}][${threadid:padding=-3}] ${level:padding=-5} ${logger} ${message} ${exception:format=tostring}"/>  </targets>
  <rules>
    <logger name="Debug" minlevel="Debug" writeTo="Debug"/>  
  </rules>
</nlog>